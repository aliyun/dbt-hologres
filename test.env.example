# Hologres Connection Configuration for Testing
# Copy this file to test.env and fill in your actual values

# Hologres instance configuration
DBT_HOLOGRES_HOST=hgxxx-xx-xxx-xx-xxx.hologres.aliyuncs.com
DBT_HOLOGRES_PORT=80
DBT_HOLOGRES_USER='BASIC$dbt_user'
DBT_HOLOGRES_PASSWORD='password'
DBT_HOLOGRES_DATABASE='db_name'
DBT_HOLOGRES_SCHEMA='public'

# Enable integration tests
DBT_HOLOGRES_RUN_INTEGRATION_TESTS=true

# Connection settings
DBT_HOLOGRES_THREADS=4
DBT_HOLOGRES_CONNECT_TIMEOUT=10
DBT_HOLOGRES_SSLMODE=disable

# Note: Username and password are case-sensitive!
